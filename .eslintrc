{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "$": "readonly",
    "App": "writable",
    "ymaps": "readonly",
    "BX": "readonly",
    "grecaptcha": "readonly"
  },
  "rules": {
    "curly": "error",
    "indent": ["error", 4, {
      "SwitchCase": 1
    }],
    "no-unused-vars": "warn"
  }
}